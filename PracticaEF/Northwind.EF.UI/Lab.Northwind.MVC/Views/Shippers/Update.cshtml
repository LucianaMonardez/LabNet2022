@model Lab.Northwind.MVC.Models.ShipperView

@{
    ViewBag.Title = "Shipper Page";
}

<h1>Actualizar Shipper</h1>
@using (Html.BeginForm("Update", "Shippers", FormMethod.Post))
{
    <label>Company name: </label> @Html.TextBoxFor(x => x.CompanyName, new { @Value = Model.CompanyName })
    <label>Phone: </label> @Html.TextBoxFor(x => x.Phone, new { @Value = Model.Phone })
    <input type="submit" class="btn btn-primary" value="Guardar" />
}
